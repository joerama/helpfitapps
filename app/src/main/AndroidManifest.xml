<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ramajoe.helpfitapps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="HELPFit Training List"
            android:theme="@style/AppTheme" />
        <activity android:name=".AddTrainingActivity" />
        <activity
            android:name=".TrainerHistory"
            android:label="Training Session History"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MemberHistory"
            android:label="Training Session History"
            android:theme="@style/AppTheme" />
        <activity android:name=".ReviewActivity"></activity>
    </application>

</manifest>